  "icon": "resources/logo.png",
  "version": "1.2.2",
  "license": "MIT",
  "main": "./extension",
  "publisher": "RimuruChan",
  "author": {
    "name": "RimuruChan",
    "email": "RealSprite233@outlook.com"
  },
  "bugs": {
    "url": "https://github.com/RimuruChan/vscode-fix-checksums/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RimuruChan/vscode-fix-checksums"
  },
  "engines": {
